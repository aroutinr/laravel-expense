{
    "name": "aroutinr/laravel-expense",
    "description": "Manage the expenses generated by your services",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Aroutin",
            "email": "raroutin@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AroutinR\\Expense\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AroutinR\\Expense\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AroutinR\\Expense\\ExpenseServiceProvider"
            ],
            "aliases": {
                "Expense": "AroutinR\\Expense\\Facades\\Expense",
                "ExpensePayment": "AroutinR\\Expense\\Facades\\ExpensePayment"
            }
        }
    },
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "orchestra/testbench": ">=4.0",
        "phpunit/phpunit": ">=8.5",
        "laravel/legacy-factories": "^1.1"
    }
}
